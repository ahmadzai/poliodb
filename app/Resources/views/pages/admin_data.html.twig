{% extends 'template.html.twig' %}
{% block left_nav %}
    <div id="left">
        <div class="subnav">
            <div class="subnav-title">
                <a href="#" class='toggle-subnav'><i class="icon-angle-down"></i><span>Remaining Children</span></a>
            </div>
            <ul class="subnav-menu">
                {% for reg in region %}
                    <li>
                        <a href="{{ path('admin_data_region', {'region':reg}) }}">
                        {% if reg == 'CR' %}
                            Central Region
                            {% elseif reg == 'ER' %}
                            East Region
                            {% elseif reg == 'SER' %}
                            South East Region
                            {% elseif reg == 'SR' %}
                            South Region
                            {% elseif reg == 'WR' %}
                            West Region
                            {% elseif reg == 'NER' %}
                            North East Region
                            {% elseif reg == 'NR' %}
                            North Region
                        {% endif %}
                        </a>
                    </li>
                {% endfor %}
            </ul>
        </div>
        <div class="subnav">
            <div class="subnav-title">
                <a href="#" class='toggle-subnav'><i class="icon-angle-down"></i><span>Vaccine Usage</span></a>
            </div>
            <ul class="subnav-menu">
                <li>
                    <a href="#">South Region</a>
                </li>
                <li>
                    <a href="#">East Region</a>
                </li>
                <li>
                    <a href="#">South East Region</a>
                </li>
                <li>
                    <a href="#">Central Region</a>
                </li>
                <li>
                    <a href="#">West Region</a>
                </li>
            </ul>
        </div>

    </div>

{% endblock %}
