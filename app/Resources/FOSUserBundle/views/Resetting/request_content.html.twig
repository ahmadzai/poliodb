</head>

{% block content %}

<body class='login'>
	<div class="wrapper">
		<h1><a href=""><img src="{{asset('img/logo-big.png')}}" alt="" class='retina-ready' width="59" height="49">LOGIN</a></h1>
		<div class="login-body">
			<h2>Request Password</h2>



<form action="{{ path('fos_user_resetting_send_email') }}" method="POST" class="fos_user_resetting_request">



        <div class="control-group">

        <label for="username">{{ 'resetting.request.username'|trans }}</label>
        <input type="text" id="username" name="username" required="required" />

    <div class="submit">
    <div class="remember">
        <input type="submit" value="{{ 'resetting.request.submit'|trans }}" class='btn btn-primary' />

        </div>
    </div>

</form>

<div>
  <a><font color="white">.</font></a>
</div>
</div>
</div>
